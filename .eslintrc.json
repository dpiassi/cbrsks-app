{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "quotes": ["error", "double"],
    "consistent-return": "off",
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "import/no-anonymous-default-export": "off"
  }
}
